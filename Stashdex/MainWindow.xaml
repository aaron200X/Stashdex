    <Grid Margin="0,0,0,0">
        <Image x:Name="stashBackground" HorizontalAlignment="Left" Height="569" Margin="355,188,0,0" VerticalAlignment="Top" Width="569" Source="D:\Programme\visual_Studio Projekte\Stashdex\Stashdex\pics\StashPanelGrid.png" Stretch="Fill" StretchDirection="UpOnly"/>
